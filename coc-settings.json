{
  "colors.enable": false,
  "translator.engines": ["youdao", "haici"],
  "Lua.telemetry.enable": true,
  "semanticTokens.enable": true,
  "sumneko-lua.enableNvimLuaDev": true,
  "sumneko-lua.locale": "zh-cn",
  "Lua.runtime.version": "LuaJIT",
  "Lua.workspace.maxPreload": 2000,
  "Lua.workspace.preloadFileSize": 1000,
  "Lua.completion.callSnippet": "Replace",
  "Lua.diagnostics.globals": [
    "vim",
    "jit",
    "it",
    "describe",
    "before_path",
    "after_each"
  ],
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "codeLens.enable": true,
  "codeLens.separator": "",
  "codeLens.subseparator": " │ ",
  "codeLens.position": "top",
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.detailField": "preview",
  "suggest.completionItemKindLabels": {
    "default": "",
    "text": "",
    "method": "",
    "function": "ƒ",
    "constructor": "",
    "field": "料",
    "variable": "",
    "class": "",
    "interface": "",
    "module": "",
    "property": "",
    "unit": "",
    "value": "",
    "enum": "",
    "keyword": "",
    "snippet": "",
    "color": "",
    "file": "",
    "reference": "渚",
    "folder": "",
    "enumMember": "",
    "constant": "",
    "struct": "פּ",
    "event": "鬒",
    "operator": "Ψ",
    "typeParameter": ""
  },
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder",
    "highlight": "Normal",
    "maxWidth": 120,
    "focusable": false
  },
  "hover.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder",
    "highlight": "Normal",
    "maxWidth": 120,
    "focusable": false
  },
  "signature.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder",
    "highlight": "Normal",
    "maxWidth": 120,
    "focusable": false
  },
  "dialog.floatHighlight": "Normal",
  "dialog.floatBorderHighlight": "Normal",
  "markdown-preview-enhanced.scrollSync": true,
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh", "zsh"],
      "ignoredRootPaths": ["~"]
    },
    "swift": {
      "command": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
      "args": [],
      "filetypes": ["swift"],
      "initializationOptions": {},
      "settings": {}
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    }
  },
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "email": "languagetool",
    "markdown": ["write-good"],
    "dockerfile": "hadolint",
  }
}
